{
  "name": "bifrost",
  "displayName": "Bifrost",
  "description": "proxy,mock,local file web server",
  "icon": "imgs/logo.png",
  "version": "0.0.11",
  "publisher": "benqy",
  "engines": {
    "vscode": "^1.10.0"
  },
  "categories": [
    "Debuggers"
  ],
  "keywords": [
    "proxy",
    "mock",
    "network"
  ],
  "main": "./out/src/extension",
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "menus": {
      "explorer/context": [
        {
          "command": "extension.bf.open_in_browser",
          "group": "navigation@1"
        },
        {
          "when": "explorerViewletVisible",
          "command": "extension.bf.proxy_this_file",
          "group": "navigation@1"
        }
      ]
    },
    "commands": [
      {
        "command": "extension.bf.open_in_browser",
        "title": "open in browser"
      },
      {
        "command": "extension.bf.proxy_this_file",
        "title": "proxy this file"
      },
      {
        "command": "extension.bf.toggle_global_proxy",
        "title": "toggle global proxy setting"
      }
    ],
    "keybindings": [
      {
        "command": "extension.bf.open_in_browser",
        "key": "shift+alt+o"
      },
      {
        "command": "extension.bf.proxy_this_file",
        "key": "shift+alt+p"
      }
    ],
    "configuration": {
        "type": "object",
        "title": "bifrost config",
        "properties": {
            "bifrost.webServerPort": {
                "type": "number",
                "default": 10086,
                "description": "local webserver port"
            },
            "bifrost.proxyServerPort": {
                "type": "number",
                "default": 17174,
                "description": "proxy server port"
            }
        }
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "http-proxy": "^1.16.2",
    "ip": "^1.1.5",
    "open": "0.0.5"
  }
}